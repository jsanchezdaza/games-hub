{
  "rewrites": [
    {
      "source": "/connecta4/(.*)",
      "destination": "https://connecta4-internal.vercel.app/$1"
    },
    {
      "source": "/connecta4",
      "destination": "https://connecta4-internal.vercel.app/"
    },
    {
      "source": "/quest-forge/(.*)",
      "destination": "https://quest-forge-internal.vercel.app/$1"
    },
    {
      "source": "/quest-forge",
      "destination": "https://quest-forge-internal.vercel.app/"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}