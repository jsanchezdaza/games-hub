{
  "rewrites": [
    {
      "source": "/connect4/(.*)",
      "destination": "https://conecta4-javisans-projects.vercel.app"
    },
    {
      "source": "/connect4",
      "destination": "https://conecta4-javisans-projects.vercel.app"
    },
    {
      "source": "/quest-forge/(.*)",
      "destination": "https://quest-forge-javisans-projects.vercel.app"
    },
    {
      "source": "/quest-forge",
      "destination": "https://quest-forge-javisans-projects.vercel.app"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}